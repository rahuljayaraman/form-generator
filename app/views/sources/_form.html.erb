<%= simple_form_for(@source) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :set_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create Data Source", class: "btn btn-primary" %>
    <%= link_to 'Back', sources_path, class: "btn" %>
  </div>
<% end %>
