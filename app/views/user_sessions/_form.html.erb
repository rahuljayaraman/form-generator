<%= form_tag user_sessions_path, :method => :post do %>
  <div class="form-inputs">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
  </div>
    <div class="form-inputs">
      <hr>
      <%= submit_tag "   Login   ", class: "btn btn-primary" %>
    </div>
<% end %>
